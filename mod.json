{
	"geode": "4.10.0",
	"gd": {
		"win": "2.2074"
	},
	"id": "beat.mega_hack_themes",
	"name": "Mega Hack Themes",
	"version": "v9.0.9-alpha.3",
	"developer": "Beat",
	"description": "Personalize Mega Hack v9.",
	"dependencies": {
		"absolllute.megahack": ">=9.0.9"
	},
	"links": {
		"source": "https://github.com/BeatACVR/geode-megahack-themes"
	},
	"tags": [
		"customization",
		"interface",
		"utility"
	],
	"settings": {
		"palette-saver": {
			"type": "custom:palette-saver",
			"name": "Palette Saver"
		},
		"main-accent": {
			"type": "rgb",
			"name": "Main Accent Color",
			"default": "#e83866",
			"requires-restart": true
		},
		"background-accent": {
			"type": "rgb",
			"name": "Background Color",
			"default": "#2a2a2a",
			"requires-restart": true
		},
		"tabtext-accent": {
			"type": "rgb",
			"name": "Tab Text Color",
			"default": "#ffffff",
			"requires-restart": true
		},
		"light-mode": {
			"type": "bool",
			"name": "Enable Light Mode",
			"description": "Changes the label text color to black.",
			"default": false,
			"requires-restart": true
		}
	}
}




